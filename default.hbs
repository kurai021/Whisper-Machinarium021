    <script type="text/javascript" src="{{asset "assets/syntaxhighlighter/scripts/shBrushSass.js"}}"></script>
    <script type="text/javascript" src="{{asset "assets/syntaxhighlighter/scripts/shBrushCss.js"}}"></script>
