    <script type="text/javascript" src="{{asset "assets/syntaxhighlighter/scripts/shBrushRuby.js"}}"></script>
    <script type="text/javascript" src="{{asset "assets/syntaxhighlighter/scripts/shBrushJava.js"}}"></script>
    <script type="text/javascript" src="{{asset "assets/syntaxhighlighter/scripts/shBrushGroovy.js"}}"></script>
    <script type="text/javascript" src="{{asset "assets/syntaxhighlighter/scripts/shBrushScala.js"}}"></script>

    <script type="text/javascript">SyntaxHighlighter.all();</script>

    {{! SumoMe Integration }}
    <script src="//load.sumome.com/" data-sumo-site-id="639ba3286c03cb72c207ad3aa61c468978aa3bc4857244d09058a889f2d5a0cd" async></script>

</head>
<body>
<a href="https://plus.google.com/+MazharAhmedPro?rel=author" style="display:none;" id="g-author">author</a>
    {{! Everything else gets inserted here }}
    {{{body}}}

    {{! The footer on the homepage}}
    <footer class="site-footer" {{#if @blog.cover}}style="background-image: url({{@blog.cover}})"{{/if}}>
        <a class="subscribe icon-feed" href="{{@blog.url}}/rss/"><span class="tooltip">Suscribete!</span></a>
        <div class="inner inner-footer">

             <section class="copyright">
	      <a rel="license" href="http://creativecommons.org/licenses/by-sa/4.0/">
		<img alt="Licencia Creative Commons" style="border-width:0" src="https://i.creativecommons.org/l/by-sa/4.0/88x31.png" />
	      </a>
	      <br />
	      <a target="_blank" href="{{@blog.url}}/">{{@blog.title}}</a> está distribuido bajo una </section>
	      <a target="_blank" rel="license" href="http://creativecommons.org/licenses/by-sa/4.0/">Licencia Creative Commons Atribución-CompartirIgual 4.0 Internacional</a>.
             <section class="poweredby"><a href="https://github.com/kurai021/Whisper-Machinarium021" target="_blank">Whisper Machinarium021</a> theme for <a target="_blank" href="https://www.facebook.com/richard.armuelles">Richard Armuelles</a> under Apache License.</section>
